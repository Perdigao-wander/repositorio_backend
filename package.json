{
   "name": "typeorm_nodejs",
   "version": "1.0.0",
   "main": "index.js",
   "scripts": {
      "build": "rm -rf ./dist && tsc",
      "star": "node ./dist/server.js",
      "build:auto-backup": "tsc ./src/scripts/backupDB.ts",
      "run:auto-backup": "node ./src/scripts/backupDB.js",
      "dev:server": "ts-node-dev --transpile-only src/server.ts",
      "typeorm": "ts-node-dev node_modules/.bin/typeorm",
      "migration-create": "node src/scripts/migration.create.js",
      "migration:generate": "typeorm-ts-node-commonjs -d ./src/data-source.ts migration:generate ./src/migrations/default",
      "migration:run": "typeorm-ts-node-commonjs -d ./src/data-source.ts migration:run",
      "migration:revert": "typeorm-ts-node-commonjs -d ./src/data-source.ts migration:revert",
      "seed:run": "ts-node ./node_modules/typeorm-extension/bin/cli.cjs -d ./src/data-source.ts seed:run",
      "seed:create": "ts-node ./node_modules/typeorm-extension/bin/cli.cjs seed:create"
   },
   "keywords": [],
   "author": "",
   "license": "ISC",
   "description": "",
   "dependencies": {
      "axios": "^1.7.9",
      "bcrypt": "^5.1.1",
      "body-parser": "^1.19.1",
      "cookie-parser": "^1.4.7",
      "cors": "^2.8.5",
      "date-fns": "^4.1.0",
      "express": "^4.21.1",
      "express-async-errors": "^3.1.1",
      "express-fileupload": "^1.5.1",
      "express-session": "^1.17.3",
      "fs": "^0.0.1-security",
      "i": "^0.3.7",
      "jsonwebtoken": "^9.0.2",
      "node-cron": "^3.0.3",
      "npm": "^11.0.0",
      "pdfkit": "^0.16.0",
      "pg": "^8.13.1",
      "puppeteer": "^24.1.0",
      "readline-sync": "^1.4.10",
      "reflect-metadata": "^0.2.2",
      "ts-node": "^10.9.2",
      "typeorm": "^0.3.20",
      "typeorm-extension": "^3.6.3",
      "uuid": "^11.0.2",
      "zod": "^3.23.8"
   },
   "devDependencies": {
      "@types/bcrypt": "^5.0.2",
      "@types/cors": "^2.8.17",
      "@types/express": "^5.0.0",
      "@types/jsonwebtoken": "^9.0.7",
      "@types/node": "^22.8.7",
      "@types/node-cron": "^3.0.11",
      "@types/pdfkit": "^0.13.8",
      "@types/uuid": "^10.0.0",
      "ts-loader": "^9.5.1",
      "ts-node-dev": "^2.0.0",
      "typescript": "^5.6.3",
      "webpack": "^5.95.0",
      "webpack-dev-server": "^5.1.0"
   }
}
